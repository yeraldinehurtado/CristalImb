@model CristalImb.Web.ViewModels.ServiciosInmueble.ServiciosInmueblesViewModel

@{
    ViewData["Title"] = "RegistrarServiciosInmueble";
    Layout = null;
}


<div class="row">
    <div class="registrar col-md-12">
        <form asp-action="RegistrarServiciosInmueble">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="col-md-12 row">
                <div class="form-group col-md-12">
                    <label asp-for="Nombre" class="control-label"></label>
                    <input asp-for="Nombre" class="form-control" />
                    <span asp-validation-for="Nombre" class="text-danger"></span>
                </div>
            </div>

            <div class="form-group">
                <input type="submit" value="Registrar" class="btn btn-primary" />
            </div>
            <div>
                <a asp-action="IndexServiciosInmueble">Volver</a>
            </div>
        </form>

    </div>
</div>




@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}