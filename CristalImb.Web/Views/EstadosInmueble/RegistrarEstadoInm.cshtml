@model CristalImb.Model.Entities.EstadosInmueble

@{
    ViewData["Title"] = "RegistrarEstadoInm";
    Layout = null;
}


<div class="row">
    <div class="col-md-10">
        <div class="registrar">
            <form asp-action="RegistrarEstadoInm">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="NombreEstado" class="control-label"></label>
                    <input asp-for="NombreEstado" class="form-control" />
                    <span asp-validation-for="NombreEstado" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Registrar" class="btn btn-primary" />
                </div>
            </form>
            <div>
                <a asp-action="IndexEstadosInm">Volver</a>
            </div>
        </div>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}