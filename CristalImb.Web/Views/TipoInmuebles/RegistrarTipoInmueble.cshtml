@model CristalImb.Web.ViewModels.TipoInmuebles.TipoInmueblesViewModel

@{
    ViewData["Title"] = "RegistrarTipoInmueble";
    Layout = null;
}
<div class="row">
    <div class="registrar col-md-12">
        <form asp-action="RegistrarTipoInmueble">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="col-md-12 row">
                <div class="form-group col-md-12">
                    <label asp-for="NombreTipoInm" class="control-label">Nombre <b class="text-danger">*</b></label>
                    <input asp-for="NombreTipoInm" class="form-control" />
                    <span asp-validation-for="NombreTipoInm" class="text-danger"></span>
                </div>
            </div>

            <div class="form-group">
                <input type="submit" value="Registrar" class="btn btn-primary" />
            </div>
            <div>
                <a asp-action="IndexTipoInmuebles">Volver</a>
            </div>
        </form>

    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}